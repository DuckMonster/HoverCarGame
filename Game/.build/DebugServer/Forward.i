typedef unsigned int uint;

namespace Hash
{
	const uint FNV_PRIME = 0x01000193;
	const uint FNV_SEED = 0x811C9DC5;

	inline uint FNV( unsigned char byte, uint hash = FNV_SEED )
	{
		return ( byte ^ hash ) * FNV_PRIME;
	}

	inline uint FNV( const void* data, size_t size, uint hash = FNV_SEED )
	{
		unsigned char* cData = (unsigned char*)data;
		while ( size-- )
		{
			hash = FNV( *cData++, hash );
		}

		return hash;
	}

	template<typename T>
	inline uint FNV( const T& data ) { return FNV( &data, sizeof( T ) ); }
}










namespace Debug
{
	__forceinline void ConsoleLogV( const char* file, long line, const char* format, va_list list )
	{
		
		const char* filePtr = file;
		int lastSplit = 0;
		int i = 0;

		while ( *filePtr )
		{
			if ( *filePtr == '\\' || *filePtr == '/' )
				lastSplit = i;

			++i;
			++filePtr;
		}

		filePtr = file + lastSplit + 1;
		

		static char buffer[4096];
		int cursor = 0;

		
		cursor += sprintf_s( buffer, 4096, "(%s:%d)> \t", filePtr, line );

		
		cursor += vsprintf_s( buffer + cursor, 4096 - cursor, format, list );

		
		buffer[cursor] = '\n';
		buffer[cursor + 1] = '\0';

		
		printf( buffer );
		
		OutputDebugStringA( buffer );
	}

	__forceinline void ConsoleLog( const char* file, long line, const char* format, ... )
	{
		
		va_list args;
		((void)(__vcrt_va_start_verify_argument_type<decltype(format)>(), ((void)(args = (va_list)(&const_cast<char&>(reinterpret_cast<const volatile char&>(format))) + ((sizeof(format) + sizeof(int) - 1) & ~(sizeof(int) - 1))))));
		ConsoleLogV( file, line, format, args );
		((void)(args = (va_list)0));
	}

	__forceinline bool CheckEnsureOnce( const char* file, long line, const char* exprStr )
	{
		static std::set<size_t> sEnsureSet;

		struct EnsureHashable
		{
			char fileName[20];
			long line;
			char expr[20];
		} hashable;

		memcpy( hashable.fileName, file, (((20) < (strlen( file ))) ? (20) : (strlen( file ))) );
		memcpy( hashable.expr, exprStr, (((20) < (strlen( exprStr ))) ? (20) : (strlen( exprStr ))) );
		hashable.line = line;

		uint hash = Hash::FNV( hashable );

		if ( sEnsureSet.find( hash ) == sEnsureSet.end() )
		{
			sEnsureSet.emplace( hash );
			return true;
		}

		return false;
	}

	__forceinline bool DoEnsure( bool expr, const char* file, long line, const char* exprStr )
	{
		if ( !expr )
		{
			ConsoleLog( file, line, "ENSURE: %s", exprStr );

			if ( CheckEnsureOnce( file, line, exprStr ) )
			{
				if ( IsDebuggerPresent() )
					DebugBreak();
				else
				{
					static char buffer[4096];
					sprintf_s( buffer, 4096, "Ensure hit at [ %s : %d ]!\n\n%s", file, line, exprStr );
					MessageBoxA( 0, buffer, "Ensure", 0x00000000L );
				}
			}
		}

		return expr;
	}

	__forceinline bool DoEnsure_Msg( bool expr, const char* file, long line, const char* exprStr, const char* msgFormat, ... )
	{
		if ( !expr )
		{
			static char sBuffer[4096];

			va_list vl;
			((void)(__vcrt_va_start_verify_argument_type<decltype(msgFormat)>(), ((void)(vl = (va_list)(&const_cast<char&>(reinterpret_cast<const volatile char&>(msgFormat))) + ((sizeof(msgFormat) + sizeof(int) - 1) & ~(sizeof(int) - 1))))));
			vsprintf_s( sBuffer, 4096, msgFormat, vl );
			((void)(vl = (va_list)0));

			ConsoleLog( file, line, "ENSURE: %s || %s", exprStr, sBuffer );

			if ( CheckEnsureOnce( file, line, exprStr ) )
			{
				if ( IsDebuggerPresent() )
					DebugBreak();
				else
				{
					static char buffer[4096];
					sprintf_s( buffer, 4096, "Ensure hit at [ %s : %d ]!\n\n%s\nMsg: %s", file, line, exprStr, sBuffer );
					MessageBoxA( 0, buffer, "Ensure", 0x00000000L );
				}
			}
		}

		return expr;
	}
}


















