	template <typename T, precision P, template <typename, precision> class vecType>
	 inline T length(vecType<T, P> const & v)
	{
		static_assert(std::numeric_limits<T>::is_iec559, "'length' only accept floating-point inputs");

		return sqrt(dot(v, v));
